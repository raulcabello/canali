    subgraph cluster2 {
        label = "Legend" ;
        shape = rectangle ;
        color = black ;
        rancher [style=filled, fillcolor="#ffcccc"] ;
        "external without s390x binary" [style=filled, fillcolor="#ccccff"] ;
